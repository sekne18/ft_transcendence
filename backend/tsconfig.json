{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 22",
	"_version": "22.15.0",
	"include": [
		"src/**/*.ts",
		"src/**/*.js"
	],
	"exclude": [
		"node_modules",
		"dist",
		"test",
		"tests",
		"**/*.test.ts",
		"**/*.test.js"
	],
	"compilerOptions": {
		"lib": [
			"ES2020"
		],
		"outDir": "dist",
		"rootDir": "src",
		"module": "nodenext",
		"target": "ES2020",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "nodenext"
	}
}